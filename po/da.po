msgstr "Bemærk: Tasterne er versalfølsomme! Dvs. at 's' og 'S' ikke det samme."

#: src/help.c:56
msgid "General movement:"
msgstr "Almindelig bevægelse:"

#: src/help.c:57
msgid ""
"  n                  Go to the next unread article (or next group, if at "
"end)."
msgstr ""
"  n                  Gå til næste ulæste indlæg (eller næste gruppe v/enden)."

#: src/help.c:58
msgid "  p                  Go to the previous unread article."
msgstr "  p                  Gå til forrige ulæste indlæg."

#: src/help.c:59
msgid "  N, ESC RIGHT       Skip to next group."
msgstr "  N, ESC RIGHT       Hop til næste gruppe."

#: src/help.c:60
msgid "  ESC LEFT           Go to previous group."
msgstr "  ESC LEFT           Gå til forrige gruppe."

#: src/help.c:61
msgid "  !                  Go to the next article with a high score."
msgstr "  !                  Gå til næste indlæg med god bedømmelse."

#: src/help.c:62
msgid "  =                  Go to the next article with the same subject."
msgstr "  =                  Gå til næste indlæg med samme emne."

#: src/help.c:63
msgid "  L                  Go to the last read article and display it."
msgstr "  L                  Gå til det sidst læste indlæg og vis det."

#: src/help.c:64 src/help.c:180
msgid "Actions:"
msgstr "Handlinger:"

#: src/help.c:65
msgid "  P                  Post a new article (no followup)."
msgstr "  P                  Send et nyt indlæg (ikke en opfølgning)."

#: src/help.c:66
msgid "  ESC P              Post or edit a postponed article."
msgstr "  ESC P              Send eller redigér et udsat indlæg."

#: src/help.c:67
msgid "  f                  Post a followup to the current article."
msgstr "  f                  Send en opfølgning til det aktuelle indlæg."

#: src/help.c:68
msgid "      ESC 1 f        Include all headers in the followup."
msgstr "      ESC 1 f        Medtag alle hoveder i opfølgningen."

#: src/help.c:69
msgid ""
"      ESC 2 f        Followup without modifying (e.g. quoting) the article."
msgstr "      ESC 2 f        Følg op uden overhovedet at ændre indlægget."

#: src/help.c:70
msgid "  r                  Reply to poster (via email)."
msgstr "  r                  Svar til forfatter (med e-post)."

#: src/help.c:71
msgid ""
"  F                  Forward the current article to someone (via email)."
msgstr ""
"  F                  Videresénd det aktuelle indlæg til nogen (med e-post)."

#: src/help.c:72
msgid ""
"      ESC 1 F        Forward the current article (including all headers)."
msgstr ""
"      ESC 1 F        Videresénd det aktuelle indlæg (inklusive alle hoveder)."

#: src/help.c:73
msgid "  ESC Ctrl-S         Supersede article (you have to be the author)."
msgstr "  ESC Ctrl-S         Erstat indlæg (du skal være forfatteren)."

#: src/help.c:74
msgid "  ESC Ctrl-C         Cancel article (you have to be the author)."
msgstr "  ESC Ctrl-C         Annullér indlæg (du skal være forfatteren)."

#: src/help.c:75
msgid "  o                  Save article, tagged articles or thread to file."
msgstr "  o                  Gem indlæg, afmærkede indlæg eller tråd til fil."

#: src/help.c:76
msgid "  |                  Pipe article to an external program."
msgstr "  |                  Send indlæg i rør til eksternt program."

#: src/help.c:77
msgid "  y                  Print article (as displayed)."
msgstr "  y                  Udskriv indlæg (som vist)."

#: src/help.c:78
msgid ""
"      ESC 1 y        Print article (unwrapped and including hidden lines)."
msgstr ""
"      ESC 1 y        Udskriv indlæg (uombrudt og inklusive skjulte linjer)."

#: src/help.c:80
msgid "  :                  Decode article, tagged articles or thread."
msgstr "  :                  Afkod indlæg, afmærkede indlæg eller tråd."

#: src/help.c:82
msgid "  Ctrl-Z             Suspend slrn."
msgstr "  Ctrl-Z             Afbryd slrn midlertidigt."

#: src/help.c:83
msgid "  q                  Return to group display."
msgstr "  q                  Vend tilbage gruppevisning."

#: src/help.c:84
msgid "  Q                  Quit slrn immediately."
msgstr "  Q                  Afslut slrn øjeblikkeligt"

