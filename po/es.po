#: src/help.c:60
msgid "  N, ESC RIGHT       Skip to next group."
msgstr "  N, ESC DERECHA     Ir al siguiente grupo."

#: src/help.c:61
msgid "  ESC LEFT           Go to previous group."
msgstr "  ESC IZQUIERDA      Ir al grupo anterior."

#: src/help.c:62
msgid "  !                  Go to the next article with a high score."
msgstr "  !                  Ir al siguiente artículo con un puntaje alto."

#: src/help.c:63
msgid "  =                  Go to the next article with the same subject."
msgstr "  =                  Ir al siguiente artículo con igual asunto."

#: src/help.c:64
msgid "  L                  Go to the last read article and display it."
msgstr "  L                  Ir y mostrar el último artículo leído"

#: src/help.c:65 src/help.c:182
msgid "Actions:"
msgstr "Acciones:"

#: src/help.c:66
msgid "  P                  Post a new article (no followup)."
msgstr "  P                  Publicar un nuevo artículo (no responder)."

#: src/help.c:67
msgid "  ESC P              Post or edit a postponed article."
msgstr "  ESC P              Publicar o editar un artículo pendiente."

#: src/help.c:68
msgid "  f                  Post a followup to the current article."
msgstr "  f                  Publicar una respuesta al artículo actual."

#: src/help.c:69
msgid "      ESC 1 f        Include all headers in the followup."
msgstr "      ESC 1 f        Incluir todas las cabeceras en la respuesta."

#: src/help.c:70
msgid ""
"      ESC 2 f        Followup without modifying (e.g. quoting) the article."
msgstr "      ESC 2 f        Responder sin modificar (citando) el artículo."

#: src/help.c:71
msgid "  r                  Reply to poster (via email)."
msgstr "  r                  Responder al autor (por correo electrónico)."

#: src/help.c:72
msgid ""
"  F                  Forward the current article to someone (via email)."
msgstr ""
"  F                  Reenviar el artículo actual a alguien (por correo "
"electrónico)."

#: src/help.c:73
msgid ""
"      ESC 1 F        Forward the current article (including all headers)."
msgstr ""
"      ESC 1 F        Reenviar el artículo actual (incluyendo todas las "
"cabeceras)."

