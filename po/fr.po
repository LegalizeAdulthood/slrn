#: src/help.c:58
msgid ""
"  n                  Go to the next unread article (or next group, if at "
"end)."
msgstr ""
"  n               Aller à l'article non lu suivant (ou au forum suivant si à "
"la fin)."

#: src/help.c:59
msgid "  p                  Go to the previous unread article."
msgstr "  p               Aller à l'article non lu précédent."

#: src/help.c:60
msgid "  N, ESC RIGHT       Skip to next group."
msgstr "  N, ESC DROITE   Passer au forum suivant."

#: src/help.c:61
msgid "  ESC LEFT           Go to previous group."
msgstr "  ESC GAUCHE      Aller au forum précédent."

#: src/help.c:62
msgid "  !                  Go to the next article with a high score."
msgstr "  !               Aller à l'article suivant ayant un score élevé."

# OTH à revoir
#: src/help.c:63
msgid "  =                  Go to the next article with the same subject."
msgstr "  =               Passer à l'article suivant de même sujet."

#: src/help.c:64
msgid "  L                  Go to the last read article and display it."
msgstr "  L               Retourner au dernier article lu."

#: src/help.c:65 src/help.c:179
msgid "Actions:"
msgstr ""

#: src/help.c:66
msgid "  P                  Post a new article (no followup)."
msgstr "  P               Publier un nouvel article (PAS un suivi-à)."

#: src/help.c:67
msgid "  ESC P              Post or edit a postponed article."
msgstr "  ESC P           Publier ou éditer un article mis en attente."

#: src/help.c:68
msgid "  f                  Post a followup to the current article."
msgstr "  f               Publier une suite à l'article en cours."

#: src/help.c:69
msgid "      ESC 1 f        Include all headers in the followup."
msgstr "      ESC 1 f     Inclure tous les en-têtes dans le suivi."

#: src/help.c:70
msgid ""
"      ESC 2 f        Followup without modifying (e.g. quoting) the article."
msgstr ""
"      ESC 2 f     Faire suivre l'article sans modifier (ex. inclure) "
"l'article.."

#: src/help.c:71
msgid "  r                  Reply to poster (via email)."
msgstr "  r               Répondre à l'auteur (par messagerie)."

#: src/help.c:72
msgid ""
"  F                  Forward the current article to someone (via email)."
msgstr "  F               Renvoyer l'article à quelqu'un (par messagerie)."

#: src/help.c:73
msgid ""
"      ESC 1 F        Forward the current article (including all headers)."
msgstr ""
"      ESC 1 F     Renvoyer l'article à quelqu'un (y compris tous les en-"
"têtes)."

#: src/help.c:74
msgid "  ESC Ctrl-S         Supersede article (you have to be the author)."
msgstr "  ESC Ctrl-S      Remplacer l'article (vous devez en être l'auteur)."

#: src/help.c:75
msgid "  ESC Ctrl-C         Cancel article (you have to be the author)."
msgstr "  ESC Ctrl-C      Annuler l'article (vous devez en être l'auteur)."

#: src/help.c:76
msgid "  o                  Save article, tagged articles or thread to file."
msgstr ""
"  o               Sauvegarder l'article, les articles marqués, ou l'enfilade."

#: src/help.c:77
msgid "  |                  Pipe article to an external program."
msgstr "  |               Envoyer (tube) l'article à une commande externe."

#: src/help.c:78
msgid "  y                  Print article (as displayed)."
msgstr "  y               Imprimer l'article (tel qu'à l'affichage)."

#: src/help.c:79
msgid ""
"      ESC 1 y        Print article (unwrapped and including hidden lines)."
msgstr ""
"      ESC 1 y     Imprimer article (sans pliage et avec les lignes cachées)."

#: src/help.c:81
msgid "  :                  Decode article, tagged articles or thread."
msgstr ""
"  :               Décoder l'article, les articles marqués, ou l'enfilade."

#: src/help.c:83
msgid "  Ctrl-Z             Suspend slrn."
msgstr "  Ctrl-Z          Suspendre slrn."

#: src/help.c:84
msgid "  q                  Return to group display."
msgstr "  q               Retourner à la liste de forums."

#: src/help.c:85
