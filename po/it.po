#: src/help.c:56
msgid "General movement:"
msgstr "Movimenti generali:"

#: src/help.c:57
msgid ""
"  n                  Go to the next unread article (or next group, if at "
"end)."
msgstr ""
"  n                  Vai al prossimo articolo da leggere (o al prossimo "
"gruppo, se alla fine)."

#: src/help.c:58
msgid "  p                  Go to the previous unread article."
msgstr "  p                  Va all'articolo non letto precedente."

#: src/help.c:59
msgid "  N, ESC RIGHT       Skip to next group."
msgstr "  N, ESC DESTRA      Vai al prossimo gruppo."

#: src/help.c:60
msgid "  ESC LEFT           Go to previous group."
msgstr "  ESC SINISTRA       Vai al gruppo precedente."

#: src/help.c:61
msgid "  !                  Go to the next article with a high score."
msgstr "  !                  Vai al prossimo articolo con un alto punteggio."

#: src/help.c:62
msgid "  =                  Go to the next article with the same subject."
msgstr "  =                  Vai al prossimo articolo con lo stesso oggetto."

#: src/help.c:63
msgid "  L                  Go to the last read article and display it."
msgstr "  L                  Vai al prossimo articolo letto e mostralo."

#: src/help.c:64 src/help.c:180
msgid "Actions:"
msgstr "Azioni:"

#: src/help.c:65
msgid "  P                  Post a new article (no followup)."
msgstr "  P                  Invia un nuovo articolo (niente followup)."

#: src/help.c:66
msgid "  ESC P              Post or edit a postponed article."
msgstr "  ESC P              Invia o edita un articolo salvato."

#: src/help.c:67
msgid "  f                  Post a followup to the current article."
msgstr "  f                  Invia un followup all'articolo corrente."

#: src/help.c:68
msgid "      ESC 1 f        Include all headers in the followup."
msgstr "      ESC 1 f        Includi tutti gli header nel followup."

#: src/help.c:69
msgid ""
"      ESC 2 f        Followup without modifying (e.g. quoting) the article."
msgstr ""
"      ESC 2 f        Rispondi senza modificare (cioè senza quotare) "
"l'articolo."

#: src/help.c:70
msgid "  r                  Reply to poster (via email)."
msgstr "  r                  Rispondi all'autore (via email)."

#: src/help.c:71
msgid ""
"  F                  Forward the current article to someone (via email)."
msgstr ""
"  F                  Inoltra l'articolo corrente a qualcuno (via email)."

#: src/help.c:72
msgid ""
"      ESC 1 F        Forward the current article (including all headers)."
msgstr ""
"      ESC 1 F        Inoltra l'articolo corrente (includendo gli header)."

#: src/help.c:73
msgid "  ESC Ctrl-S         Supersede article (you have to be the author)."
msgstr "  ESC Ctrl-S         Modifica l'articolo (devi essere l'autore)."

#: src/help.c:74
msgid "  ESC Ctrl-C         Cancel article (you have to be the author)."
msgstr "  ESC Ctrl-C         Cancella l'articolo (devi essere l'autore)."

#: src/help.c:75
msgid "  o                  Save article, tagged articles or thread to file."
msgstr ""
"  o                  Salva l'articolo, gli articoli segnati o il thread su "
"un file."

#: src/help.c:76
msgid "  |                  Pipe article to an external program."
msgstr ""
"  |                  Invia l'articolo ad un programma esterno tramite un pipe"

#: src/help.c:77
msgid "  y                  Print article (as displayed)."
msgstr "  y                  Stampa l'articolo (come è visualizzato)."

#: src/help.c:78
msgid ""
"      ESC 1 y        Print article (unwrapped and including hidden lines)."
msgstr ""
"      ESC 1 y        Stampa l'articolo (senza margini e con le linee "
"nasoste)."

#: src/help.c:80
msgid "  :                  Decode article, tagged articles or thread."
msgstr ""
"  :                  Decodifica l'articolo, gli articoli segnati o il thread."

#: src/help.c:82
msgid "  Ctrl-Z             Suspend slrn."
msgstr "  Ctrl-Z             Sospendi slrn."

#: src/help.c:83
msgid "  q                  Return to group display."
msgstr "  q                  Ritorna ai gruppi."

#: src/help.c:84
msgid "  Q                  Quit slrn immediately."
msgstr "  Q                  Termina slrn immediatamente."

